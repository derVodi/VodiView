<UserControl x:Class="CommandBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VodiView"
             mc:Ignorable="d" 
             >

	<StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top">
		<Button x:Name="gShowInfo" Content="  ℹ  " Background="Transparent" Foreground="White" FontSize="18.667" HorizontalAlignment="Left" VerticalAlignment="Top"/>
		<Button x:Name="gNormalize" Content="  ⧉  " Background="Transparent" Foreground="White" FontSize="18.667" HorizontalAlignment="Left" VerticalAlignment="Top"/>
		<Button x:Name="gClose" Content="  ✖  " Background="Transparent" Foreground="White" FontSize="18.667" HorizontalAlignment="Left" VerticalAlignment="Top"/>
	</StackPanel>


</UserControl>
