<Window x:Class="RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VodiView"
        mc:Ignorable="d"
        Title="Rename" Height="194" Width="320"
				WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow" MinWidth="320" MinHeight="194" MaxHeight="194" Activated="Window_Activated">
	<Grid Margin="0,0,0,0">
		<Label Content="Original Name" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="OriginalNameTextBox" Height="23" Margin="10,36,10,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" IsReadOnly="True"/>
		<Label Content="New Name" HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="NewNameTextBox" Height="23" Margin="10,90,10,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"/>
		<Button Content="Rename" HorizontalAlignment="Right" Margin="128,124,100,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.232,0.558" IsDefault="True" Click="Button_Click"/>
		<Button Content="Cancel" HorizontalAlignment="Right" Margin="217,124,10,0" VerticalAlignment="Top" Width="75" IsCancel="True"/>
	</Grid>
</Window>
